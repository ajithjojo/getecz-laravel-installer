{
  "name": "getecz/laravel-installer",
  "description": "Getecz Laravel Installer \u2013 lightweight installer wizard for Laravel 12 applications",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "installer",
    "wizard"
  ],
  "authors": [
    {
      "name": "Ajith Joseph"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Getecz\\Installer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Getecz\\Installer\\InstallerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}